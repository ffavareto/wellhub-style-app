{
  "name": "wellhub-style-app",
  "version": "1.0.0",
  "description": "Wellhub stype app",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "database:start": "docker compose up -d",
    "database:stop": "docker compose stop",
    "build": "tsup src --out-dir build",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:insall-prisma-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:insall-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "TypeScript",
    "Fastify",
    "SOLID",
    "Postgresql",
    "Back-End",
    "Vitest"
  ],
  "author": "Lucas Santos",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "22.7.4",
    "@vitest/coverage-v8": "2.1.2",
    "@vitest/ui": "2.1.2",
    "npm-run-all": "4.1.5",
    "prisma": "5.20.0",
    "tsup": "8.3.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vitest": "2.1.2"
  },
  "dependencies": {
    "@fastify/jwt": "9.0.1",
    "@prisma/client": "5.20.0",
    "@types/bcryptjs": "2.4.6",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "fastify": "5.0.0",
    "zod": "3.23.8"
  }
}
